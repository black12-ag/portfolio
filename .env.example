# ====================================
# üöÄ API CONFIGURATION
# ====================================
# Backend API URLs (Required for booking/payment flows)
VITE_BACKEND_BASE_URL=http://localhost:3001
VITE_API_BASE_URL=http://localhost:3001/api
VITE_API_VERSION=v1
VITE_API_TIMEOUT=10000

# WebSocket for real-time features
VITE_WS_BASE_URL=ws://localhost:3001

# ====================================
# üè® HOTEL DATA API (LiteAPI Integration)
# ====================================
# LiteAPI for hotel/travel data (Get from https://liteapi.travel)
# Environment: 'sandbox' for testing, 'production' for live data
VITE_LITEAPI_ENV=sandbox

# Sandbox Keys (for development/testing)
VITE_LITEAPI_SANDBOX_PRIVATE_KEY=sand_55e7f623-4408-471b-ba99-a7cd48fdb135
VITE_LITEAPI_SANDBOX_PUBLIC_KEY=sand_public_123

# Production Keys (for live deployment)
VITE_LITEAPI_PRODUCTION_PRIVATE_KEY=prod_4aeaebc2-f3b9-43c2-83b6-a67f03df3f19
VITE_LITEAPI_PRODUCTION_PUBLIC_KEY=prod_public_456

# LiteAPI Feature Flags
VITE_ENABLE_LITEAPI=true
VITE_LITEAPI_CACHE_TIMEOUT=1800000

# ====================================
# üîê AUTHENTICATION & SECURITY
# ====================================
VITE_JWT_SECRET=dev-jwt-secret-key-for-development-only
