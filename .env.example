VITE_APP_ENV=development
VITE_BUILD_VERSION=1.0.0-dev
VITE_BUILD_DATE=2024-01-01

# Performance & Optimization
VITE_ENABLE_SERVICE_WORKER=false
VITE_CACHE_MAX_AGE=300
