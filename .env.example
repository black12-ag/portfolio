# WhatsApp Business (Get from https://business.whatsapp.com)
VITE_WHATSAPP_BUSINESS_ID=your-whatsapp-business-id

# ====================================
# üéØ FEATURE FLAGS (Development)
# ====================================
VITE_ENABLE_CHAT=true
VITE_ENABLE_VOICE_SEARCH=false
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PWA=true
VITE_ENABLE_GEOLOCATION=true
VITE_ENABLE_BIOMETRIC_AUTH=false
VITE_ENABLE_LITEAPI_DEV=true

# ====================================
